    "crud-mongoose": "^1.0.32",
    "currency-formatter": "^1.3.1",
    "dockernode": "^0.1.0",
    "dockerode": "^2.5.3",
    "fifo": "^2.3.0",
    "lodash": "^4.17.4",
    "mongodb": "^2.2.33",
    "mongoose": "^4.12.4",
    "node-id3": "^0.1.0",
    "node-schedule": "^1.2.5",
    "shelljs": "^0.7.8",
    "telegraf": "^3.13.0",
    "telegraf-command-parts": "^1.0.3",
    "telegraf-flow": "^8.3.0",
    "telegraf-session-local": "0.0.4",
    "telegraf-wit": "^2.1.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-es2015": "^6.24.1"
  }
}
