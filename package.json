  "main": "index.js",
  "scripts": {
    "test3": "jest",
    "test5": "karma start",
    "lint": "eslint -c .eslintrc app",
    "test": "ava",
    "test-node": "./node_modules/mocha/bin/mocha $(find api -name '*-test.js') --compilers js:babel-core/register",
    "test-node-watch": "./node_modules/mocha/bin/mocha $(find api -name '*-test.js') --compilers js:babel-core/register --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Empia/flownotes-chrome.git"
  },
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Empia/flownotes-chrome/issues"
  },
  "ava": {
    "files": ["tests/*.js"],
    "source": ["bot/**/*.{js,jsx}"],
    "concurrency": 5,
    "failFast": true,
    "failWithoutAssertions": false,
    "tap": false
  },
  "dependencies": {
    "@std/esm": "^0.13.0",
    "agenda": "^1.0.3",
    "ansi-escape-sequences": "^4.0.0",
