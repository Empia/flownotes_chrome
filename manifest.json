{
  "manifest_version": 2,
  "background": {
      "page": "popup.html"
   },

  "name": "Getting started example",
  "description": "This extension shows a Google Image search result for the current page",
  "version": "1.0",
  "options_page": "popup.html",


  "browser_action": {
    "default_icon": "icon.png"
  },
  "content_security_policy": "script-src 'self' http://192.168.253.219:3000; object-src 'self'",

  "permissions": [
    "storage",
	"tabs"
  ]
}
